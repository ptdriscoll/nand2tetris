/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not (in=sel, out=notsel);
    And (a=a, b=notsel, out=selx);
    And (a=sel, b=b, out=sely);
    Or (a=selx, b=sely, out=out);
}